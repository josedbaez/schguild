    <PackageReleaseNotes>v0.0.1.3 - This version contains Status API, some Option List API, and Querying API calls.</PackageReleaseNotes>
    <PackageLicenseUrl>https://github.com/sitecoreguild/schguild/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryType>GitHub</RepositoryType>
    <RepositoryUrl>https://github.com/sitecoreguild/schguild</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

</Project>
